<%= form_with(model:@feed, local:true, url:feeds_path) do |form| %>
  <%= form.hidden_field :content %>
  <p>content:<%= @feed.content %>
  <%= form.submit %>
<% end %>

<%= form_with(model:@feed, local:true, url: new_feed_path, method: 'get') do |form| %>
  <%= form.hidden_field :content %>
  <%= form.submit 'return', name: 'back' %>
<% end %>
